{
	"name": "pexisgle",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "bun run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "bun run scripts/drizzle-local.ts migrate",
		"db:migrate:prod": "drizzle-kit migrate",
		"db:studio": "bun run scripts/drizzle-local.ts studio",
		"db:studio:prod": "drizzle-kit studio",
		"deploy": "bun run build && wrangler deploy",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@iconify/svelte": "^5.2.1",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.3",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.0.3",
		"arktype": "^2.1.29",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"flowbite": "^4.0.1",
		"flowbite-svelte": "^1.31.0",
		"flowbite-svelte-admin-dashboard": "^2.1.1",
		"flowbite-svelte-icons": "^3.1.0",
		"globals": "^16.5.0",
		"highlight.js": "^11.11.1",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"ress": "^5.0.2",
		"sass": "^1.97.2",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"sveltekit-superforms": "^2.29.1",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0",
		"vite": "^7.3.1",
		"wrangler": "^4.57.0"
	},
	"dependencies": {
		"@cartamd/plugin-code": "^4.2.0",
		"@cartamd/plugin-emoji": "^4.3.0",
		"@cartamd/plugin-slash": "^4.2.0",
		"arctic": "^3.7.0",
		"carta-md": "^4.11.1",
		"dayjs": "^1.11.19",
		"drizzle-arktype": "^0.1.3",
		"gsap": "^3.14.2",
		"rehype-highlight": "^7.0.2",
		"svelte-exmarkdown": "^5.0.2",
		"svelte-meta-tags": "^4.5.0",
		"uuid": "^13.0.0"
	}
}
